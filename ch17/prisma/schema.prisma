// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Episode {
  id         Int      @id @default(autoincrement())
  name       String
  air_date   String
  episode    String
  characters String
  url        String
  created    DateTime @default(now())
} 